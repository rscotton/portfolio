{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "member-access": false,
    "ordered-imports": false,
    "no-console": false,
    "no-var-requires": false,
    "quotemark": [true, "single", "jsx-double"],
    "arrow-parens": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}
